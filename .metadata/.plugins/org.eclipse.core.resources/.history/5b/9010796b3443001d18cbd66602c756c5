package aula1Dio.tiposDeDados;

public class TipoDeDados {
	
	public static void main(String[] args) {
		
		byte b1 = 10;
		byte b2 = 20;
		
		short s1 = 20000;
		//ultrapassou limite byte
		//short s2 = 40000;
		
		//ultrapassou limite int
		//int i1 = -10000000000;
		int i2 = 28500;
		
		long l1 = 1000000000000000000L;
		long l2 = 2004000500000000000L;
		
		//float f1 = 4.5;
		float f2 = 10.68f;
		
		double d1 = 85.69;
		double d2 = 99.84d;
		
		
	}

}
