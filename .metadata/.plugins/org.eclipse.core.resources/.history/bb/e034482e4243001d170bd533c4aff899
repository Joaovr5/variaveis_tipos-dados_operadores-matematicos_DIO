package aula1Dio.cast;

public class cast {

	public static void main(String[] args) {
		
		byte b1;
		short s1 = 1000;
		b1 = (byte) s1;
		
		long l1;
		int i1 = 10;
		l1 = i1;
		
		int i2;
		long l2 = 1000000000000000000L;
		i2 = (int) l2;
		
		double d1;
		float f1 = 10.5f;
		d1 = f1;
		
		

	}

}
